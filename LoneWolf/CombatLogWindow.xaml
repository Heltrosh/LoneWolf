<Window x:Name="windowCBLog" x:Class="LoneWolf.CombatLogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoneWolf"
        mc:Ignorable="d"
        Title="Combat Log" Height="470" Width="450" Loaded="windowCBLog_loaded">
    <Grid>
        <Button x:Name="btnPrev" Content="Prev" HorizontalAlignment="Left" Margin="10,404,0,0" VerticalAlignment="Top" Click="btnPrev_Click"/>
        <Button x:Name="btnNext" Content="Next" HorizontalAlignment="Left" Margin="400,404,0,0" VerticalAlignment="Top" Click="btnNext_Click"/>
        <TextBlock x:Name="txtCombatLog" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="389" Width="419"/>

    </Grid>
</Window>
